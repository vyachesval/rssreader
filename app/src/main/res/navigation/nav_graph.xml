<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragment_rsschannels_list">

    <fragment
        android:id="@+id/fragment_rsschannels_list"
        android:name="dev.rssreader.presentation.screen.list.RssChannelsListFragment"
        android:label="@string/rsschannels_list_fragment_label"
        tools:layout="@layout/fragment_rsschannels_list">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="dev.rssreader.presentation.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/fragment_rsschannels_list" />
    </fragment>
</navigation>